    "@nestjs/websockets": "^5.6.0",
    "@ngx-formly/core": "^5.0.0-beta.23",
    "@ngx-formly/material": "^5.0.0-beta.23",
    "@ngx-lite/in-viewport": "^0.1.3",
    "@ngx-lite/input-star-rating": "^0.2.5",
    "@ngx-lite/input-tag": "^0.2.8",
    "@ngxs-labs/immer-adapter": "^1.1.0",
    "@ngxs/devtools-plugin": "^3.3.0",
    "@ngxs/form-plugin": "^3.3.0",
    "@ngxs/router-plugin": "^3.3.0",
    "@ngxs/storage-plugin": "^3.3.0",
    "@ngxs/store": "^3.3.0",
    "@xmlking/angular-oauth2-oidc-all": "^5.1.0",
    "@xmlking/api-ai-javascript": "^2.0.0-beta.22",
    "@xmlking/jwks-rsa": "^1.4.3",
    "@xmlking/ngx-knob": "0.1.0",
