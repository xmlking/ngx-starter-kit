{
  "name": "ngx-starter-kit",
  "version": "3.1.0",
  "description": "Angular-NestJS Starter Kit Monorepo",
  "author": "Sumanth Chinthagunta <xmlking@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xmlking/ngx-starter-kit.git"
  },
  "workspaces": [
    "libs/*",
    "apps/*",
    "tools"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:mock": "ng serve -c=mock",
    "// - BUILDING": "Build Apps for Prod, Mock environments",
    "prebuild": "rimraf dist coverage",
    "build": "ng build",
    "build:prod": "ng build webapp -c=production",
    "deploy:mock": "ng deploy --base-href=/ngx-starter-kit/ --configuration=mock",
    "bundle-report": "ng build -c=production --stats-json && webpack-bundle-analyzer dist/apps/webapp/stats-es2015.json",
    "report-coverage": "codecov --token=$CODECOV_TOKEN",
    "// - RELEASE": "release Apps and Libs",
    "commit": "git-cz",
    "commit:lint": "commitlint -E HUSKY_GIT_PARAMS",
    "semantic-release": "semantic-release",
    "// - DOCS": "Generate and serve docs",
    "doc:build": "compodoc -p tsconfig.json -d dist/docs",
    "doc:serve": "compodoc -s -d dist/docs",
    "doc:build-serve": "compodoc -p tsconfig.json -d dist/docs -s",
    "// - TEST": "Test unit and e2e",
    "test": "ng test webapp",
    "test:ci": "ng test webapp --ci --max-workers=4",
    "test:affected": "yarn affected:test --uncommitted",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e webapp",
    "e2e:ci": "ng e2e webapp --headless",
    "// - AFFECTED": "work on only affected modules",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:deploy": "nx affected:deploy",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "// - FORMAT": "Format and lint Utils",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "nx": "nx"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{apps,libs}/**/*.{ts,json,md,scss,html}": [
      "yarn affected:lint --uncommitted --parallel --fix=true",
      "yarn format:write --uncommitted"
    ]
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/github"
    ],
    "generateNotes": {
      "preset": "angular"
    },
    "npmPublish": false
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "8.2.14",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/pwa": "0.803.23",
    "@angular/router": "8.2.14",
    "@angular/service-worker": "8.2.14",
    "@fortawesome/angular-fontawesome": "0.5.0",
    "@fortawesome/fontawesome-svg-core": "1.2.26",
    "@fortawesome/free-brands-svg-icons": "5.12.0",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
    "@ngx-formly/core": "5.5.10",
    "@ngx-formly/material": "5.5.10",
    "@ngx-lite/in-viewport": "0.1.3",
    "@ngx-lite/input-star-rating": "0.3.5",
    "@ngx-lite/input-tag": "0.4.8",
    "@ngxs-labs/immer-adapter": "3.0.5",
    "@ngxs/devtools-plugin": "3.6.1",
    "@ngxs/form-plugin": "3.6.1",
    "@ngxs/logger-plugin": "3.6.1",
    "@ngxs/router-plugin": "3.6.1",
    "@ngxs/storage-plugin": "3.6.1",
    "@ngxs/store": "3.6.1",
    "@xmlking/api-ai-javascript": "2.0.0-beta.22",
    "@xmlking/ngx-knob": "0.3.0",
    "@xmlking/ngx-quicklink": "0.0.11",
    "angular-in-memory-web-api": "0.9.0",
    "angular-oauth2-oidc": "8.0.4",
    "chart.js": "2.9.3",
    "chart.piecelabel.js": "0.15.0",
    "core-js": "3.6.4",
    "d3": "4.13.0",
    "d3-selection-multi": "1.0.1",
    "date-fns": "2.9.0",
    "filepond": "4.9.5",
    "filepond-plugin-file-encode": "2.1.4",
    "filepond-plugin-file-validate-size": "2.2.0",
    "filepond-plugin-file-validate-type": "1.2.4",
    "filepond-plugin-image-crop": "2.0.3",
    "filepond-plugin-image-exif-orientation": "1.0.6",
    "filepond-plugin-image-preview": "4.6.0",
    "filepond-plugin-image-resize": "2.0.4",
    "filepond-plugin-image-transform": "3.7.0",
    "formidable": "1.2.1",
    "hammerjs": "2.0.8",
    "immer": "5.1.0",
    "intersection-observer": "0.7.0",
    "keycloak-js": "8.0.1",
    "ngx-auto-unsubscribe": "3.0.1",
    "ngx-filepond": "5.0.1",
    "ngx-page-scroll": "7.0.0",
    "ngx-page-scroll-core": "7.0.0",
    "ngx-perfect-scrollbar": "8.0.0",
    "rxjs": "6.5.4",
    "screenfull": "5.0.1",
    "smooth-scrollbar": "8.5.1",
    "socket.io-client": "2.3.0",
    "tslib": "1.10.0",
    "uuid": "3.4.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.23",
    "@angular-devkit/build-ng-packagr": "0.803.23",
    "@angular/cli": "8.3.23",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@nrwl/angular": "8.11.2",
    "@nrwl/nest": "8.11.2",
    "@nrwl/workspace": "8.11.2",
    "@types/google.analytics": "0.0.40",
    "@types/hammerjs": "2.0.36",
    "@types/jest": "24.9.1",
    "@types/jquery": "3.3.31",
    "@types/node": "13.5.0",
    "@types/socket.io-client": "1.4.32",
    "codecov": "3.6.2",
    "codelyzer": "5.2.1",
    "loaders.css": "0.1.2",
    "ng-packagr": "5.7.1",
    "ngx-deploy-npm": "1.2.0",
    "rimraf": "3.0.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "tsickle": "0.38.0",
    "tslint": "5.20.1",
    "typescript": "3.5.3"
  },
  "optionalDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@compodoc/compodoc": "1.1.11",
    "@ngx-formly/schematics": "5.5.10",
    "@ngxs/schematics": "0.0.1-alpha.5",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/git": "8.0.0",
    "@semantic-release/github": "6.0.2",
    "@semantic-release/npm": "6.0.0",
    "angular-cli-ghpages": "0.6.2",
    "commitizen": "4.0.3",
    "cypress": "3.8.3",
    "cz-conventional-changelog": "3.0.2",
    "dotenv": "8.2.0",
    "husky": "4.2.1",
    "jest": "25.1.0",
    "jest-preset-angular": "7.1.1",
    "lint-staged": "10.0.3",
    "lite-server": "2.5.4",
    "prettier": "1.19.1",
    "semantic-release": "16.0.4",
    "ts-jest": "25.0.0",
    "webpack-bundle-analyzer": "3.6.0"
  }
}
