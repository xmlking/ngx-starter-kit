  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular-devkit/build-ng-packagr": "~0.1000.1",
    "@angular/cli": "10.0.8",
    "@angular/compiler-cli": "10.0.10",
    "@angular/language-service": "10.0.10",
    "@compodoc/compodoc": "*",
    "@ngx-builders/analyze": "^2.0.0",
    "@nrwl/angular": "9.5.1",
    "@nrwl/nest": "9.5.1",
    "@nrwl/workspace": "9.5.1",
    "@twittwer/compodoc": "^1.3.1",
    "@types/google.analytics": "0.0.40",
    "@types/jest": "26.0.10",
    "@types/jquery": "3.5.1",
    "@types/node": "^14.0.14",
    "@types/socket.io-client": "1.4.33",
    "codecov": "3.7.2",
    "codelyzer": "^6.0.0",
    "loaders.css": "0.1.2",
    "ng-packagr": "^10.0.0",
    "ngx-deploy-npm": "1.2.2",
    "rimraf": "3.0.2",
    "ts-loader": "7.0.5",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "tslint": "~6.1.0",
    "typescript": "3.9.7"
  },
  "optionalDependencies": {
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@compodoc/compodoc": "1.1.11",
    "@ngx-formly/schematics": "5.9.3",
    "@ngxs/schematics": "0.0.1-alpha.5",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.0.7",
    "@semantic-release/npm": "7.0.5",
    "angular-cli-ghpages": "0.6.2",
    "commitizen": "4.1.2",
    "cypress": "4.12.1",
    "cz-conventional-changelog": "3.2.0",
    "dotenv": "8.2.0",
    "husky": "4.2.5",
    "jest": "26.4.1",
    "jest-preset-angular": "8.2.1",
    "lint-staged": "10.2.11",
    "lite-server": "2.5.4",
    "prettier": "2.0.5",
    "semantic-release": "17.1.1",
    "ts-jest": "26.2.0"
  },
  "resolutions": {
    "d3": "4.13.0",
    "d3-selection": "1.4.2",
    "immer": "5.3.6"
  }
}
