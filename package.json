    "@ngx-lite/input-tag": "0.4.10",
    "@ngxs-labs/immer-adapter": "3.0.5",
    "@ngxs/devtools-plugin": "3.7.2",
    "@ngxs/form-plugin": "3.7.2",
    "@ngxs/logger-plugin": "3.7.2",
    "@ngxs/router-plugin": "3.7.2",
    "@ngxs/storage-plugin": "3.7.2",
    "@ngxs/store": "3.7.2",
    "@xmlking/api-ai-javascript": "2.0.0-beta.22",
    "@xmlking/ngx-knob": "1.1.1",
    "angular-in-memory-web-api": "0.11.0",
    "angular-oauth2-oidc": "10.0.3",
    "chart.js": "3.4.1",
    "chart.piecelabel.js": "0.15.0",
    "core-js": "3.15.2",
    "d3": "6.7.0",
    "d3-selection-multi": "1.0.1",
    "date-fns": "2.22.1",
    "filepond": "4.28.2",
    "filepond-plugin-file-encode": "2.1.10",
    "filepond-plugin-file-validate-size": "2.2.4",
    "filepond-plugin-file-validate-type": "1.2.6",
    "filepond-plugin-image-crop": "2.0.6",
    "filepond-plugin-image-exif-orientation": "1.0.11",
    "filepond-plugin-image-preview": "4.6.6",
    "filepond-plugin-image-resize": "2.0.10",
    "filepond-plugin-image-transform": "3.7.6",
    "formidable": "1.2.2",
