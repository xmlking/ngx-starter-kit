{
  "name": "@ngx-starter-kit/api",
  "version": "0.0.0",
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register --project apps/api/tsconfig.app.json node_modules/.bin/typeorm",
    "typeorm:sync": "typeorm schema:sync",
    "typeorm:seeds": "typeorm migration:run",
    "typeorm:flush": "typeorm migration:revert",
    "typeorm:create": "typeorm migration:create",
    "typeorm:preserve": "yarn typeorm:sync -f=ormconfig && yarn typeorm:seeds -f=ormconfig",
    "start": "ts-node -r tsconfig-paths/register --project apps/api/tsconfig.app.json src/main.ts",
    "build": "ng build api",
    "build:prod": "ng build api --prod"
  },
  "dependencies": {
    "@godaddy/terminus": "4.4.0",
    "@nestjs/common": "7.0.9",
    "@nestjs/core": "7.0.9",
    "@nestjs/cqrs": "6.1.0",
    "@nestjs/passport": "7.0.0",
    "@nestjs/platform-express": "7.0.9",
    "@nestjs/platform-socket.io": "7.0.9",
    "@nestjs/swagger": "4.5.3",
    "@nestjs/terminus": "7.0.1",
    "@nestjs/typeorm": "7.0.0",
    "@nestjs/websockets": "7.0.9",
    "@nestjsx/crud": "4.5.0",
    "@xmlking/jwks-rsa": "1.4.3",
    "cache-manager": "3.2.1",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "cli-ux": "4.9.3",
    "cls-hooked": "4.2.2",
    "helmet": "3.22.0",
    "kubernetes-client": "8.3.7",
    "nest-router": "1.0.9",
    "nodemailer": "6.4.6",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "pg": "8.0.3",
    "pug": "2.0.4",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5",
    "sharp": "0.24.1",
    "swagger-ui-express": "4.1.4",
    "typeorm": "0.2.24",
    "typescript": "~3.8.3",
    "web-push": "3.4.3"
  },
  "devDependencies": {
    "@types/helmet": "0.0.46",
    "@types/nodemailer": "6.4.0",
    "@types/passport": "1.0.3",
    "@types/passport-jwt": "3.0.3",
    "@types/socket.io": "2.1.4",
    "@types/supertest": "2.0.8",
    "@types/web-push": "3.3.0"
  },
  "optionalDependencies": {
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.9",
    "nodemon": "2.0.3",
    "supertest": "4.0.2"
  }
}
