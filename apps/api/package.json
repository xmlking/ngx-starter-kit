{
  "name": "@ngx-starter-kit/api",
  "version": "0.0.0",
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register --project apps/api/tsconfig.app.json node_modules/.bin/typeorm",
    "typeorm:sync": "typeorm schema:sync",
    "typeorm:seeds": "typeorm migration:run",
    "typeorm:flush": "typeorm migration:revert",
    "typeorm:create": "typeorm migration:create",
    "typeorm:preserve": "yarn typeorm:sync -f=ormconfig && yarn typeorm:seeds -f=ormconfig",
    "start": "ts-node -r tsconfig-paths/register --project apps/api/tsconfig.app.json src/main.ts",
    "build": "ng build api",
    "build:prod": "ng build api --prod"
  },
  "dependencies": {
    "@godaddy/terminus": "4.9.0",
    "@nestjs/common": "8.0.5",
    "@nestjs/core": "8.0.5",
    "@nestjs/axios": "0.0.1",
    "@nestjs/microservices": "8.0.5",
    "@nestjs/platform-express": "8.0.5",
    "@nestjs/swagger": "5.0.8",
    "@nestjs/cqrs": "8.0.0",
    "@nestjs/passport": "8.0.0",
    "@nestjs/platform-socket.io": "8.0.5",
    "@nestjs/terminus": "7.2.0",
    "@nestjs/typeorm": "8.0.1",
    "@nestjs/websockets": "8.0.5",
    "@nestjsx/crud": "5.0.0-alpha.3",
    "@xmlking/jwks-rsa": "1.4.3",
    "cache-manager": "3.4.4",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "cli-ux": "5.6.3",
    "cls-hooked": "4.2.2",
    "helmet": "4.6.0",
    "kubernetes-client": "9.0.0",
    "nest-router": "1.0.9",
    "nodemailer": "6.6.3",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "pg": "8.7.0",
    "pug": "3.0.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0",
    "sharp": "0.28.3",
    "swagger-ui-express": "4.1.6",
    "typeorm": "0.2.37",
    "typescript": "4.3.5",
    "web-push": "3.4.5"
  },
  "devDependencies": {
    "@types/helmet": "4.0.0",
    "@types/nodemailer": "6.4.4",
    "@types/passport": "1.0.7",
    "@types/passport-jwt": "3.0.6",
    "@types/socket.io": "3.0.2",
    "@types/supertest": "2.0.11",
    "@types/web-push": "3.3.2"
  },
  "optionalDependencies": {
    "@nestjs/schematics": "8.0.2",
    "@nestjs/testing": "8.0.5",
    "nodemon": "2.0.12",
    "supertest": "6.1.4"
  }
}
