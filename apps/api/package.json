{
  "name": "@ngx-starter-kit/api",
  "version": "0.0.0",
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register --project apps/api/tsconfig.app.json node_modules/.bin/typeorm",
    "typeorm:sync": "typeorm schema:sync",
    "typeorm:seeds": "typeorm migration:run",
    "typeorm:flush": "typeorm migration:revert",
    "typeorm:create": "typeorm migration:create",
    "typeorm:preserve": "yarn typeorm:sync -f=ormconfig && yarn typeorm:seeds -f=ormconfig",
    "start": "ts-node -r tsconfig-paths/register --project apps/api/tsconfig.app.json src/main.ts",
    "build": "ng build api",
    "build:prod": "ng build api --prod"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.2.0",
    "@nestjs/common": "^6.8.2",
    "@nestjs/core": "^6.8.2",
    "@nestjs/cqrs": "^6.0.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.8.2",
    "@nestjs/platform-socket.io": "^6.8.2",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/terminus": "^6.5.2",
    "@nestjs/typeorm": "^6.2.0",
    "@nestjs/websockets": "^6.8.2",
    "@nestjsx/crud": "^4.2.0",
    "@xmlking/jwks-rsa": "^1.4.3",
    "cache-manager": "^2.10.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.1",
    "cli-ux": "4.9.3",
    "cls-hooked": "^4.2.2",
    "helmet": "^3.21.1",
    "kubernetes-client": "^8.3.4",
    "nest-router": "^1.0.0",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "pug": "^2.0.4",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.5.3",
    "sharp": "^0.23.1",
    "swagger-ui-express": "^4.1.1",
    "typeorm": "^0.2.18",
    "typeorm-express-query-builder": "^1.1.3",
    "web-push": "^3.4.0"
  },
  "devDependencies": {
    "@types/helmet": "^0.0.45",
    "@types/nodemailer": "^6.2.1",
    "@types/passport": "1.0.1",
    "@types/passport-jwt": "^3.0.2",
    "@types/socket.io": "^2.1.3",
    "@types/supertest": "^2.0.8",
    "@types/web-push": "^3.3.0"
  },
  "optionalDependencies": {
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.8.2",
    "nodemon": "^1.19.3",
    "supertest": "^4.0.0"
  }
}
