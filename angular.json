                "hidden": true,
                "scripts": true,
                "styles": true
              },
              "vendorChunk": false
            },
            "production": {
              "aot": true,
              "budgets": [
                {
                  "baseline": "1.5mb",
                  "maximumError": "20%",
                  "maximumWarning": "10%",
                  "name": "vendor",
                  "type": "bundle"
                },
                {
                  "baseline": "5.5mb",
                  "maximumError": "20%",
                  "maximumWarning": "10%",
                  "minimumError": "20%",
                  "minimumWarning": "10%",
                  "type": "allScript"
                },
                {
                  "baseline": "2.5mb",
                  "maximumError": "20%",
                  "maximumWarning": "10%",
                  "type": "initial"
                },
                {
                  "baseline": "300kb",
                  "maximumError": "20%",
                  "maximumWarning": "10%",
                  "name": "polyfills",
                  "type": "bundle"
                },
                {
                  "baseline": "350kb",
                  "maximumError": "20%",
                  "maximumWarning": "10%",
                  "name": "styles",
                  "type": "bundle"
                },
                {
                  "baseline": "11mb",
                  "maximumError": "20%",
                  "maximumWarning": "10%",
                  "type": "all"
                },
                {
                  "baseline": "8mb",
                  "maximumError": "20%",
                  "maximumWarning": "10%",
                  "name": "main",
                  "type": "bundle"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "buildOptimizer": true,
              "extractCss": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "apps/webapp/src/environments/environment.ts",
                  "with": "apps/webapp/src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": false,
              "ngswConfigPath": "apps/webapp/ngsw-config.json",
              "optimization": true,
              "outputHashing": "all",
              "serviceWorker": true,
              "sourceMap": {
                "hidden": true,
                "scripts": true,
                "styles": true
              },
              "vendorChunk": false
            },
            "serve": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "extractCss": true,
              "preserveSymlinks": true
            }
          },
          "options": {
            "aot": true,
            "assets": [
              "apps/webapp/src/assets",
              "apps/webapp/src/favicon.ico",
              "apps/webapp/src/manifest.json",
              "apps/webapp/src/silent-refresh.html"
            ],
            "index": "apps/webapp/src/index.html",
            "main": "apps/webapp/src/main.ts",
            "outputPath": "dist/apps/webapp",
            "polyfills": "apps/webapp/src/polyfills.ts",
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": ["apps/webapp/src/styles"]
            },
            "styles": [
              "apps/webapp/src/styles.scss",
              {
                "bundleName": "assets/themes/deeppurple-amber",
                "input": "apps/webapp/src/styles/themes/deeppurple-amber.scss",
                "inject": false
              },
              {
                "bundleName": "assets/themes/indigo-pink",
                "input": "apps/webapp/src/styles/themes/indigo-pink.scss",
                "inject": false
              },
              {
                "bundleName": "assets/themes/pink-bluegrey",
                "input": "apps/webapp/src/styles/themes/pink-bluegrey.scss",
                "inject": false
              },
              {
                "bundleName": "assets/themes/purple-green",
