{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "webapp": {
      "root": "apps/webapp/",
      "sourceRoot": "apps/webapp/src",
      "projectType": "application",
      "prefix": "ngx",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/webapp",
            "index": "apps/webapp/src/index.html",
            "main": "apps/webapp/src/main.ts",
            "polyfills": "apps/webapp/src/polyfills.ts",
            "tsConfig": "apps/webapp/tsconfig.app.json",
            "assets": [
              "apps/webapp/src/favicon.ico",
              "apps/webapp/src/assets",
              "apps/webapp/src/manifest.json",
              "apps/webapp/src/silent-refresh.html"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["apps/webapp/src/styles"]
            },
            "styles": [
              "apps/webapp/src/styles.scss",
              {
                "input": "apps/webapp/src/styles/themes/deeppurple-amber.scss",
                "bundleName": "assets/themes/deeppurple-amber",
                "lazy": true
              },
              {
                "input": "apps/webapp/src/styles/themes/indigo-pink.scss",
                "bundleName": "assets/themes/indigo-pink",
                "lazy": true
              },
              {
                "input": "apps/webapp/src/styles/themes/pink-bluegrey.scss",
                "bundleName": "assets/themes/pink-bluegrey",
                "lazy": true
              },
              {
                "input": "apps/webapp/src/styles/themes/purple-green.scss",
                "bundleName": "assets/themes/purple-green",
                "lazy": true
              }
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/webapp/src/environments/environment.ts",
                  "with": "apps/webapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "hidden": true
              },
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true,
              "ngswConfigPath": "apps/webapp/ngsw-config.json",
              "budgets": [
                {
                  "type": "bundle",
                  "name": "polyfills",
                  "baseline": "300kb",
                  "maximumError": "20%",
                  "maximumWarning": "10%"
                },
                {
                  "type": "bundle",
                  "name": "vendor",
                  "baseline": "1.5mb",
                  "maximumError": "20%",
                  "maximumWarning": "10%"
                },
                {
                  "type": "bundle",
                  "name": "styles",
                  "baseline": "350kb",
                  "maximumError": "20%",
                  "maximumWarning": "10%"
                },
                {
                  "type": "bundle",
                  "name": "main",
                  "baseline": "10mb",
                  "maximumError": "20%",
                  "maximumWarning": "10%"
                },
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "allScript",
                  "baseline": "4.5mb",
                  "maximumError": "20%",
                  "maximumWarning": "10%",
                  "minimumError": "20%",
                  "minimumWarning": "10%"
                },
                {
                  "type": "all",
                  "baseline": "50mb",
                  "maximumError": "20%",
                  "maximumWarning": "10%"
                }
              ]
            },
            "mock": {
              "fileReplacements": [
                {
                  "replace": "apps/webapp/src/environments/environment.ts",
                  "with": "apps/webapp/src/environments/environment.mock.ts"
                }
              ],
              "baseHref": "/ngx-starter-kit/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "hidden": true
              },
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true
            },
            "serve": {
              "preserveSymlinks": true,
              "extractCss": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "webapp:build:serve"
          },
          "configurations": {
            "production": {
              "browserTarget": "webapp:build:production"
            },
            "mock": {
              "browserTarget": "webapp:build:mock"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "webapp:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/webapp/tsconfig.app.json", "apps/webapp/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "**/*.json"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/webapp/jest.config.js",
            "tsConfig": "apps/webapp/tsconfig.spec.json",
            "setupFile": "apps/webapp/src/test-setup.ts"
          }
        }
      }
    },
    "webapp-e2e": {
      "root": "apps/webapp-e2e",
      "projectType": "application",
